rrentSong} />\n      <Player \n        audioRef={audioRef}\n        setIsPlaying={setIsPlaying}\n        isPlaying={isPlaying}\n        currentSong={currentSong} \n        setSongInfo={setSongInfo}\n        songInfo={songInfo}\n      />\n      <Library \n        audioRef={audioRef} \n        songs={songs} \n        setCurrentSong={setCurrentSong} \n        isPlaying={isPlaying}\n      />\n      <audio \n        onTimeUpdate={timeUpdateHandler} \n        onLoadedMetadata={timeUpdateHandler}\n        ref={audioRef} \n        src={currentSong.audio}\n      ></audio>\n    </div>\n  );\n}\n\nexport default App;\n","C:\\Users\\bbbar\\OneDrive\\Documentos\\Coding\\Coding_folder\\dev_Ed\\the_creative_react\\chap3\\music-player-react\\src\\reportWebVitals.js",[],"C:\\Users\\bbbar\\OneDrive\\Documentos\\Coding\\Coding_folder\\dev_Ed\\the_creative_react\\chap3\\music-player-react\\src\\components\\Song.js",[],"C:\\Users\\bbbar\\OneDrive\\Documentos\\Coding\\Coding_folder\\dev_Ed\\the_creative_react\\chap3\\music-player-react\\src\\components\\Player.js",["40"],"import React, {useState} from \"react\";\r\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\r\nimport { faPlay, faPause, faAngleLeft, faAngleRight } from \"@fortawesome/free-solid-svg-icons\";\r\n\r\nconst Player = ({ currentSong, isPlaying, setIsPlaying, audioRef, setSongInfo, songInfo }) => { \r\n  //Event Handlers\r\n  const playSongHandler = () => {\r\n    if(isPlaying) {\r\n      audioRef.current.pause();\r\n      setIsPlaying(!isPlaying);\r\n    } else {\r\n      audioRef.current.play();\r\n      setIsPlaying(!isPlaying);\r\n    }\r\n  };\r\n  \r\n  const getTime = (time) => {\r\n    return(\r\n      Math.floor(time / 60) + \":\" + (\"0\" + Math.floor(time % 60)).slice(-2)\r\n    );\r\n  };\r\n  const dragHandler = (e) => {\r\n    audioRef.current.currentTime = e.target.value;\r\n    setSongInfo({...songInfo, currentTime: e.target.value});\r\n  };\r\n\r\n    \r\n\r\n  return(\r\n    <div className=\"player\">\r\n      <div className=\"time-control\">\r\n        <p>{getTime(songInfo.currentTime)}</p>\r\n        <input \r\n          min={0} \r\n          max={songInfo.duration} \r\n          value={songInfo.currentTime} \r\n          onChange={dragHandler}\r\n          type=\"range\"\r\n        />\r\n        <p>{getTime(songInfo.duration)}</p>\r\n      </div>\r\n      <div className=\"play-control\">\r\n        <FontAwesomeIcon className=\"skip-back\" size=\"2x\" icon={faAngleLeft} />\r\n        <FontAwesomeIcon onClick={playSongHandler} className=\"play\" size=\"2x\" icon={isPlaying ? faPause : faPlay} />        \r\n        <FontAwesomeIcon className=\"skip-forward\" size=\"2x\" icon={faAngleRight} />\r\n      </div>\r\n      \r\n    </div>    \r\n  );\r\n};\r\n\r\nexport default Player;","C:\\Users\\bbbar\\OneDrive\\Documentos\\Coding\\Coding_folder\\dev_Ed\\the_creative_react\\chap3\\music-player-react\\src\\data.js",[],"C:\\Users\\bbbar\\OneDrive\\Documentos\\Coding\\Coding_folder\\dev_Ed\\the_creative_react\\chap3\\music-player-react\\src\\components\\Library.js",[],"C:\\Users\\bbbar\\OneDrive\\Documentos\\Coding\\Coding_folder\\dev_Ed\\the_creative_react\\chap3\\music-player-react\\src\\components\\LibrarySong.js",[],{"ruleId":"41","replacedBy":"42"},{"ruleId":"43","replacedBy":"44"},{"ruleId":"45","severity":1,"message":"46","line":11,"column":17,"nodeType":"47","messageId":"48","endLine":11,"endColumn":25},{"ruleId":"45","severity":1,"message":"49","line":1,"column":16,"nodeType":"47","messageId":"48","endLine":1,"endColumn":24},"no-native-reassign",["50"],"no-negated-in-lhs",["51"],"no-unused-vars","'setSongs' is assigned a value but never used.","Identifier","unusedVar","'useState' is defined but never used.","no-global-assign","no-unsafe-negation"]